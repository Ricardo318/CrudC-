<Window x:Class="Examen1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Examen1"
        mc:Ignorable="d"
        Title="Examen 1" Height="450" Width="800">
    <Grid>
        <DockPanel LastChildFill="False">
            <Menu x:Name="mnPrincipal" DockPanel.Dock="Top" IsMainMenu="True" Height="20">
                <MenuItem Header="Equipo">
                    <MenuItem Header="Agregar" x:Name="Agregar" Click="Agregar_Click"/>
                    <MenuItem Header="Listar Todos" x:Name="ListarTodos" Click="ListarTodos_Click"/>
                </MenuItem>
                <MenuItem Header="Informacion">
                    <MenuItem Header="Acerca de" x:Name="Acerca_de" Click="Acerca_de_Click"/>
                </MenuItem>
            </Menu>
        </DockPanel>
    </Grid>
</Window>
